<h4>注文履歴一覧</h4>
<table>
  <thead>
    <tr>
      <td>購入日時</td>
      <td>購入者</td>
      <td>注文個数</td>
      <td>注文ステータス</td>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td>
          <%= @order.created_at %>
        </td>
        <td>
          <%= @customer.name %>
        </td>
        <td>
          <%= @customer.email %>
        </td>
        <td>
          <%= @customer.is_deleted %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @items %>